.grid-33
  .app--section
    ul.app--user--list
      each user in data.friendsList.users
        li
          .circle--fluid
            .circle--fluid--cell.circle--fluid--primary
              a.app--tweet--author
                img.app--avatar(src= user.profile_image_url)
                h4= user.name
                p @#{user.screen_name}
            .circle--fluid--cell
              a.button.button-text Unfollow